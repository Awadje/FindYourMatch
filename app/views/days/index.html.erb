<div class="panel panel-primary">
  <div class="panel-heading">
    <h1>Welcome:
      <%= current_user.email %></h1>
    <h2> Today's Match </h2>
    <% @studentdays.each do |day| %>

    <h2><%= day.day %> | <%= day.id_first_student.email %>  & <%= day.id_second_student.email %></h2>


    <% end %>

    <h2> Historical Matches</h2>
    <% @studenthistory.each do |day| %>

    <h2><%= day.day %> | <%= day.id_first_student.email %>  & <%= day.id_second_student.email %></h2>


    <% end %>


  </div>
